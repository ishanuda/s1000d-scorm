  <content>
    <learning>
      <learningAssessment>
        <title>Converter catalyst maintenance procedure - Assessment</title>
        <lcInteraction>
          <lcSingleSelect>
            <lcQuestion>
              <description>
                <para>Select your answer, and then click NEXT.</para>
                <para>What is the correct pattern for torquing the ILPE converter's cover bolts?</para>
              </description>
            </lcQuestion>
            <lcAnswerOptionGroup>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>Clockwise</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>Counterclockwise</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>90-degree cross-pattern</para>
                  </description>
                </lcAnswerOptionContent>
                <lcCorrectResponse lcName="lcCorrectResponse"></lcCorrectResponse>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>45-degree cross-pattern </para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
            </lcAnswerOptionGroup>
          </lcSingleSelect>
        </lcInteraction>
        <lcInteraction id="ia-002">
          <lcSingleSelect>
            <title></title>
            <lcQuestion>
              <description>
                <para>Select your answer, and then click NEXT.</para>
                <para>What percentage of final torque should be applied during the first pass of tightening the ILPE converter's cover bolts?</para>
              </description>
            </lcQuestion>
            <lcAnswerOptionGroup>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>20%</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>50%</para>
                  </description>
                </lcAnswerOptionContent>
                <lcCorrectResponse lcName="lcCorrectResponse"></lcCorrectResponse>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>75%</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>100%</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
            </lcAnswerOptionGroup>
          </lcSingleSelect>
        </lcInteraction>
        <lcInteraction id="ia-003">
          <lcSingleSelect>
            <title></title>
            <lcQuestion>
              <description>
                <para>Select your answer, and then click NEXT.</para>
                <para>Which maintenance procedure may fail due to improper seating of the ILPE converter's cover?</para>
              </description>
            </lcQuestion>
            <lcAnswerOptionGroup>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>ASU/RCU Drop Test</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>HP Phase Separator Drop Test</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>Purge Gas Tank Drop Test</para>
                  </description>
                </lcAnswerOptionContent>
              </lcAnswerOption>
              <lcAnswerOption>
                <lcAnswerOptionContent>
                  <description>
                    <para>BMP Drop Test</para>
                  </description>
                </lcAnswerOptionContent>
                <lcCorrectResponse lcName="lcCorrectResponse"></lcCorrectResponse>
              </lcAnswerOption>
            </lcAnswerOptionGroup>
          </lcSingleSelect>
        </lcInteraction>
